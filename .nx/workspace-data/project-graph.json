            "executor": "@storybook/angular:build-storybook",
            "cache": true,
            "outputs": [
              "{options.outputDir}"
            ],
            "inputs": [
              "default",
              "^production"
            ],
            "options": {
              "configDir": "libs/dcx-ng-lib/.storybook",
              "browserTarget": "dcx-ng-lib:build-storybook",
              "compodoc": false,
              "outputDir": "dist/storybook/dcx-ng-lib"
