            },
            "configurations": {},
            "parallelism": true
          },
          "static-storybook": {
            "executor": "@nx/web:file-server",
            "dependsOn": [
              "build-storybook"
            ],
            "options": {
              "buildTarget": "dcx-ng-lib:build-storybook",
              "staticFilePath": "dist/storybook/dcx-ng-lib",
              "spa": true
            },
            "configurations": {
              "ci": {
                "buildTarget": "dcx-ng-lib:build-storybook:ci"
              }
            },
            "parallelism": true
          },
          "start": {
            "executor": "nx:run-script",
            "options": {
              "script": "start"
            },
            "metadata": {
              "scriptContent": "nx serve",
              "runCommand": "npm run start"
            },
            "configurations": {},
            "parallelism": true
          },
          "start-storybook": {
            "executor": "nx:run-script",
            "options": {
              "script": "start-storybook"
            },
            "metadata": {
              "scriptContent": "npx nx storybook dcx-ng-lib",
              "runCommand": "npm run start-storybook"
            },
            "configurations": {},
            "parallelism": true
          },
          "lint-fix": {
            "executor": "nx:run-script",
            "options": {
              "script": "lint-fix"
            },
            "metadata": {
              "scriptContent": "nx lint dcx-ng-lib --fix",
              "runCommand": "npm run lint-fix"
            },
            "configurations": {},
            "parallelism": true
          },
          "test": {
            "executor": "@nx/jest:jest",
            "outputs": [
              "{workspaceRoot}/coverage/{projectRoot}"
            ],
            "options": {
              "jestConfig": "libs/dcx-ng-lib/jest.config.ts",
              "passWithNoTests": true
            },
            "configurations": {
              "ci": {
                "ci": true,
                "codeCoverage": true
              }
            },
            "parallelism": true,
            "cache": true,
            "inputs": [
              "default",
              "^production",
              "{workspaceRoot}/jest.preset.js"
            ]
          },
          "build": {
            "executor": "@nx/angular:package",
            "outputs": [
              "{workspaceRoot}/dist/libs/dcx-ng-lib"
            ],
            "options": {
              "project": "libs/dcx-ng-lib/ng-package.json"
            },
            "configurations": {},
            "parallelism": true
          },
          "lint": {
            "executor": "@nx/eslint:lint",
            "configurations": {},
            "options": {},
            "parallelism": true,
            "cache": true,
