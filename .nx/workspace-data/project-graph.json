            "configurations": {
              "ci": {
                "quiet": true
              }
            },
            "parallelism": true
          },
          "storybook": {
            "executor": "@storybook/angular:start-storybook",
            "options": {
              "configDir": "libs/dcx-ng-lib/.storybook",
              "browserTarget": "dcx-ng-lib:build-storybook",
              "compodoc": false,
              "port": 4400
            },
            "configurations": {
              "ci": {
                "quiet": true
              }
            },
            "parallelism": true
          },
