          "targetGroups": {
            "NPM Scripts": [
              "start",
              "start-storybook",
              "lint-fix"
            ]
          }
        },
        "$schema": "../../node_modules/nx/schemas/project-schema.json",
        "prefix": "dcx-ng",
        "projectType": "library",
        "implicitDependencies": []
      }
    },
